								<height>110</height>
								<colordiffuse>AA000000</colordiffuse>
								<texture border="20,20,20,20">menu_back.png</texture>
						</control>
						<control type="label" id="2">
								<description>coordinates label</description>
								<posx>20</posx>
								<posy>10</posy>
								<width>750</width>
								<label>-</label>
								<align>center</align>
								<font>size_23</font>
						</control>
						<control type="label" id="3">
								<description>help information</description>
								<posx>20</posx>
								<posy>40</posy>
								<width>750</width>
