						<width>1280</width>
						<height>720</height>
						<texture>gui_underlay.png</texture>
				</control>
				<control type="mover" id="8">
						<description>top left mover</description>
						<posx>0</posx>
						<posy>0</posy>
						<texturefocus>calibrate-topleft-focus.png</texturefocus>
						<texturenofocus>calibrate-topleft.png</texturenofocus>
