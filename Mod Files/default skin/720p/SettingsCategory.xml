<window id="12">
		<defaultcontrol>3</defaultcontrol>
		<allowoverlay>no</allowoverlay>
		<controls>
				<!-- Background Images -->
				<control type="image">
						<description>background</description>
						<posx>0</posx>
						<posy>0</posy>
						<width>1280</width>
						<height>720</height>
						<aspectratio>stretch</aspectratio>
						<colordiffuse>FF97999B</colordiffuse>
						<texture>background diffuse.png</texture>
				</control>
				<!-- Top Underlay Image -->
				<control type="image">
						<description>top bar</description>
						<posx>0</posx>
						<posy>0</posy>
						<width>1280</width>
						<height>115</height>
						<aspectratio>stretch</aspectratio>
						<colordiffuse>FFC8CACB</colordiffuse>
						<texture>background diffuse.png</texture>
				</control>
				<!-- System Log -->
				<control type="image">
						<description>logo</description>
						<posx>30</posx>
						<posy>20</posy>
						<width>490</width>
						<height>72</height>
						<aspectratio align="left">keep</aspectratio>
						<texture>layouts/settings/art/logo.png</texture>
				</control>
				<!-- Bottom Underlay Image -->
				<control type="image">
						<posx>0</posx>
						<posy>47r</posy>
						<width>1280</width>
						<height>47</height>
						<aspectratio>stretch</aspectratio>
						<colordiffuse>FFC5C6C7</colordiffuse>
						<texture>background diffuse.png</texture>
				</control>
				<include>ContextButtonsSettings</include>
				<control type="group">
						<include>ViewVisibleEffect</include>
						<control type="grouplist" id="5">
								<description>control area</description>
								<posx>340</posx>
								<posy>138.5</posy>
								<width>880</width>
								<height>510</height>
								<itemgap>0</itemgap>
								<pagecontrol>90</pagecontrol>
								<onleft>3</onleft>
								<onright>90</onright>
								<onup>5</onup>
								<ondown>5</ondown>
						</control>
						<control type="scrollbar" id="90">
								<posx>1225</posx>
								<posy>138.5</posy>
								<width>17</width>
								<height>510</height>
								<texturesliderbackground border="2,2,2,2">scrollbar_bar_back.png</texturesliderbackground>
								<texturesliderbar border="2,16,2,16">scrollbar_bar.png</texturesliderbar>
								<texturesliderbarfocus border="2,16,2,16">scrollbar_bar_focus.png</texturesliderbarfocus>
								<textureslidernib>scrollbar_nib.png</textureslidernib>
								<textureslidernibfocus>scrollbar_nib.png</textureslidernibfocus>
								<onleft>5</onleft>
								<onright>3</onright>
								<showonepage>false</showonepage>
								<orientation>vertical</orientation>
						</control>
				</control>
				<control type="group">
						<include>ViewVisibleEffect</include>
						<posx>40</posx>
						<posy>113.5</posy>
						<control type="image">
								<description>shadow border</description>
								<posx>-8</posx>
								<posy>17</posy>
								<width>306</width>
								<height>451</height>
								<texture border="10">border-shadow.png</texture>
						</control>
						<control type="image">
								<description>Down Arrow</description>
								<posx>30</posx>
								<posy>0</posy>
								<width>40</width>
								<height>30</height>
								<texture>arrow-big-up.png</texture>
						</control>
						<control type="image">
								<description>background image</description>
								<posx>0</posx>
								<posy>25</posy>
								<width>290</width>
								<height>435</height>
								<texture border="8">ListNF.png</texture>
						</control>
						<control type="grouplist" id="3">
								<description>button area</description>
								<posx>5</posx>
								<posy>35</posy>
								<width>280</width>
								<height>425</height>
								<itemgap>0</itemgap>
								<onleft>5</onleft>
								<onright>5</onright>
								<onup>3</onup>
								<ondown>3</ondown>
						</control>
				</control>
				<control type="button" id="10">
						<description>Default Category Button</description>
						<include>ButtonCommonValues</include>
				</control>
				<control type="button" id="7">
						<description>Default Button</description>
						<posx>0</posx>
						<posy>0</posy>
						<height>45</height>
						<font>size_20</font>
						<textcolor>white</textcolor>
						<focusedcolor>black</focusedcolor>
						<texturefocus border="5">ThumbFO.png</texturefocus>
						<texturenofocus border="5">ListNF.png</texturenofocus>
				</control>
				<control type="radiobutton" id="8">
						<description>Default RadioButton</description>
						<posx>0</posx>
						<posy>0</posy>
						<height>45</height>
						<font>size_20</font>
						<textcolor>white</textcolor>
						<focusedcolor>black</focusedcolor>
						<texturefocus border="5">ThumbFO.png</texturefocus>
						<texturenofocus border="5">ListNF.png</texturenofocus>
				</control>
				<control type="spincontrolex" id="9">
						<description>Default SpinControlex</description>
						<posx>0</posx>
						<posy>0</posy>
						<height>45</height>
						<font>size_20</font>
						<textcolor>white</textcolor>
						<focusedcolor>black</focusedcolor>
						<texturefocus border="5">ThumbFO.png</texturefocus>
						<texturenofocus border="5">ListNF.png</texturenofocus>
						<aligny>center</aligny>
						<reverse>yes</reverse>
				</control>
				<control type="image" id="11">
						<description>separator image</description>
						<height>4</height>
						<texture>-</texture>
				</control>
				<include>BehindDialogFadeOut</include>
		</controls>
</window>